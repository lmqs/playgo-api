{
   "name": "playgo-api",
   "version": "1.0.0",
   "description": "API from playgo app",
   "main": "index.ts",
   "scripts": {
      "test": "jest --passWithNoTests",
      "test:watch": "npm run test -- --watch",
      "test:staged": "npm test -- --findRelatedTests",
      "test:ci": "npm test -- --coverage",
      "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
      "test:coverage": "npm test -- --coverage --logHeapUsage",
      "build": "rimraf build && tsc -p tsconfig-build.json",
      "start:build": "npm run build && node -r dotenv/config build/index.js",
      "prepare": "husky install",
      "migrate": "ts-node-dev -r tsconfig-paths/register -r dotenv/config ./node_modules/.bin/knex --knexfile ./src/infra/service/knexfile.ts migrate:latest",
      "create:migrate": "knex migrate:make --migrations-directory ./migrations -x ts change_name_file",
      "typeorm": "typeorm-ts-node-commonjs"
   },
   "author": "Luciana Queiroz",
   "license": "ISC",
   "devDependencies": {
      "@types/amqplib": "^0.10.1",
      "@types/bcrypt": "^5.0.0",
      "@types/express": "^4.17.15",
      "@types/graphql": "^14.5.0",
      "@types/jest": "^29.2.4",
      "@types/jsonwebtoken": "^9.0.0",
      "@types/knex": "^0.16.1",
      "@types/module-alias": "^2.0.1",
      "@types/node": "^16.11.10",
      "@types/pg": "^8.6.6",
      "@types/supertest": "^2.0.12",
      "@types/swagger-ui-express": "^4.1.3",
      "@typescript-eslint/eslint-plugin": "^5.48.0",
      "@typescript-eslint/parser": "^5.48.0",
      "eslint": "^8.31.0",
      "eslint-config-standard-with-typescript": "^26.0.0",
      "eslint-plugin-import": "^2.26.0",
      "eslint-plugin-n": "^15.6.0",
      "eslint-plugin-promise": "^6.1.1",
      "husky": "^8.0.2",
      "jest": "^29.3.1",
      "lint-staged": "^13.1.0",
      "rimraf": "^4.1.2",
      "ts-jest": "^29.0.3",
      "ts-node-dev": "^2.0.0",
      "ts-standard": "^12.0.2",
      "typescript": "4.5.2",
      "ts-node": "10.7.0"
   },
   "dependencies": {
      "@types/validator": "^13.7.11",
      "amqplib": "^0.10.3",
      "apollo-server-express": "^3.12.0",
      "bcrypt": "^5.1.0",
      "dayjs": "^1.11.7",
      "dotenv": "^16.0.3",
      "env-var": "^7.3.0",
      "express": "^4.18.2",
      "graphql": "^16.6.0",
      "jsonwebtoken": "^9.0.0",
      "knex": "^2.3.0",
      "module-alias": "^2.2.2",
      "moment": "^2.29.4",
      "pg": "^8.4.0",
      "reflect-metadata": "^0.1.13",
      "swagger-ui-express": "^4.6.0",
      "tsconfig-paths": "^4.2.0",
      "typeorm": "0.3.17",
      "validator": "^13.7.0"
   }
}
