{
  "name": "playgo-api",
  "version": "1.0.0",
  "description": "API from playgo app",
  "main": "index.ts",
  "scripts": {
    "test": "jest --passWithNoTests --runInBand",
    "test:watch": "jest --watch",
    "test:staged": "npm test -- --findRelatedTests",
    "test:ci": "npm test -- --coverage",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "start": "node build/index.js",
    "build": "rimraf build && tsc -p tsconfig-build.json",
    "start:build": "npm run build && nodemon ./src/index.ts",
    "prepare": "husky install",
    "migrate": "ts-node-dev -r tsconfig-paths/register -r dotenv/config ./node_modules/.bin/knex --knexfile ./src/infra/service/knexfile.ts migrate:latest",
    "create:migrate": "knex migrate:make --migrations-directory ./migrations -x ts change_name_file"
  },
  "author": "Luciana Queiroz",
  "license": "ISC",
  "devDependencies": {
    "@types/amqplib": "^0.10.1",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.15",
    "@types/graphql": "^14.5.0",
    "@types/jest": "^29.2.4",
    "@types/jsonwebtoken": "^9.0.0",
    "@types/knex": "^0.16.1",
    "@types/node": "^18.11.16",
    "@types/pg": "^8.6.6",
    "@types/supertest": "^2.0.12",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^5.48.0",
    "@typescript-eslint/parser": "^5.48.0",
    "eslint": "^8.31.0",
    "eslint-config-standard-with-typescript": "^26.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.6.0",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^8.0.2",
    "jest": "^29.3.1",
    "lint-staged": "^13.1.0",
    "rimraf": "^4.1.2",
    "ts-jest": "^29.0.3",
    "ts-node-dev": "^2.0.0",
    "ts-standard": "^12.0.2",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "@types/validator": "^13.7.11",
    "amqplib": "^0.10.3",
    "apollo-server-express": "^3.12.0",
    "bcrypt": "^5.1.0",
    "dayjs": "^1.11.7",
    "dotenv": "^16.0.3",
    "env-var": "^7.3.0",
    "express": "^4.18.2",
    "graphql": "^16.6.0",
    "jsonwebtoken": "^9.0.0",
    "knex": "^2.3.0",
    "module-alias": "^2.2.2",
    "moment": "^2.29.4",
    "pg": "^8.8.0",
    "swagger-ui-express": "^4.6.0",
    "validator": "^13.7.0"
  },
  "_moduleAliases": {
    "@": "build"
  }
}
